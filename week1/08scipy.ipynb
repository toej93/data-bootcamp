         "source" : [
            "def integral(f, a, b, h):\n    sum = 0\n    x = a\n    while x < b:\n        sum = sum + f(x)*h\n        x = x + h\n    return sum"
         ],
         "metadata" : {},
         "execution_count" : 1,
         "outputs" : [],
         "cell_type" : "code"
      },
      {
         "source" : [
