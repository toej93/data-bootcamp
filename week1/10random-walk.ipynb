    "import random as r\n",
    "#p=0.5\n",
    "def biased_coin(p):#p is the probability that we get heads, e.g. p=1 means that the experiment will always give heads\n",
    "    u = r.random()\n",
    "    if u<p:\n",
    "        return 'H'\n",
    "    else:\n",
    "        return 'T'\n",
    "\n",
    "#biased_coin(0)\n",
    "\n",
    "def run_coin(n,p):\n",
    "    # Initialize the output of the function as an empty string\n",
    "    result = '' \n",
    "    # Loop over the range of n and add the coin toss result to the output each iteration\n",
    "    for k in range(n):\n",
    "        result = result + biased_coin(p) \n",
    "    return list(result)\n",
    "#run_coin(5)\n",
    "\n",
    "def count_heads(tosses,p):\n",
    "    List=run_coin(tosses,p)\n",
    "   # print(List)\n",
    "    n_heads=0\n",
    "    for i in range(0,len(List)):\n",
    "        if List[i]==\"T\":\n",
    "            n_heads+=1\n",
    "    #raise Exception('You must implement count_heads.')\n",
    "    return n_heads\n",
    "\n",
    "#count_heads(50)\n",
    "def coin_experiment(num_tosses,num_repeats,p):\n",
    "    results = []\n",
    "    for j in range(num_repeats):\n",
    "        results.append(count_heads(num_tosses,p))\n",
    "    return results\n",
    "def mean(L):\n",
    "    sum=0\n",
    "    for i in range(0,len(L)):\n",
    "        sum+=L[i]\n",
    "    return sum/len(L)\n",
    "    #raise Exception('You must implement mean.')\n",
    "def mean_norm(mean,n_tries):\n",
    "    return mean/n_tries\n",
    "\n",
    "mean_norm(mean(coin_experiment(10,10,0.5)),10)"
   ]
