    "def uniqued(list):\n",
    "    list=sorted(list)\n",
    "    i=0\n",
    "    length=len(list)\n",
    "    while i<length:\n",
    "        #print(i)\n",
    "        if list[i]==list[i+1]:\n",
    "            list.pop(i+1)\n",
    "            length=len(list)\n",
    "        i+=1\n",
    "    print(list)\n",
    "\n",
    "List = [\"The\", \"earth\", \"revolves\", \"around\", \"sun\", \"sun\", \"around\"]\n",
    "\n",
    "uniqued(List)\n",
